<div class="page-header">
  <h1>Event #<%= @event.id %></h1>
</div>

<dl class="dl-horizontal">
  <dt>User</dt>
  <dd><%= @user.first_name %> <%= @user.last_name %></dd>

  <dt>Cuisine</dt>
  <dd><%= @event.cuisine %></dd>

  <dt>Location</dt>
  <dd><%= @event.location %></dd>

  <dt>Event date</dt>
  <dd><%= @event.event_date %></dd>

</dl>
